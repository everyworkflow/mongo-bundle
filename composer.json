{
    "name": "everyworkflow/mongo-bundle",
    "description": "Mongo bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "ext-mongodb": "*",
        "mongodb/mongodb": "^1.10",
        "symfony/console": "6.0.*",
        "nesbot/carbon": "^2.55",
        "everyworkflow/core-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\MongoBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\MongoBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
